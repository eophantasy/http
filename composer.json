{
    "name": "eophantasy/http",
    "description": "Library to send HTTP requests",
    "type": "library",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:eophantasy/time.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:eophantasy/io.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:eophantasy/types.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Eophantasy\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eophantasy\\Http\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ilya Sitnikov",
            "email": "sitnikovik@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0",
        "eophantasy/time": "dev-master",
        "eophantasy/io": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    }
}
